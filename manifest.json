{
    "manifest_version": 2,
    "name": "LiChess Pie Clock",
    "description": "This extension shows the animation for clock on Lichess.org",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "http://lichess.org/*",
                "https://lichess.org/*"
            ],
            "js": [
                "jquery.js",
                "timer.js"
            ]
        }
    ]
}